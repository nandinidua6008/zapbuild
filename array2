int main()
{
    int a[10]={94,48,44,21,32,444,21,123,13,344},i,temp;
    printf("elements of std::array< is \n");
    for(i=0;i<10;i++){
        printf("%d ",a[i]);
    }
    temp=a[0];
    for(i=1;i<10;i++){
        if(temp<a[i]){
            temp=a[i];
        }
    }
    printf("\nlargest no is %d",temp);
    return 0;
}

